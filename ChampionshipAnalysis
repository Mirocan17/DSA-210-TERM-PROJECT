        "!pip install nba_api pandas\n",
        "import pandas as pd\n",
        "import time\n",
        "from nba_api.stats.static import teams\n",
        "from nba_api.stats.endpoints import commonteamroster\n",
        "champions_map = [\n",
        "    {'year': '2023-24', 'team_code': 'BOS'},\n",
        "    {'year': '2022-23', 'team_code': 'DEN'},\n",
        "    {'year': '2021-22', 'team_code': 'GSW'},\n",
        "    {'year': '2020-21', 'team_code': 'MIL'},\n",
        "    {'year': '2019-20', 'team_code': 'LAL'},\n",
        "    {'year': '2018-19', 'team_code': 'TOR'},\n",
        "    {'year': '2017-18', 'team_code': 'GSW'},\n",
        "    {'year': '2016-17', 'team_code': 'GSW'},\n",
        "    {'year': '2015-16', 'team_code': 'CLE'},\n",
        "    {'year': '2014-15', 'team_code': 'GSW'},\n",
        "    {'year': '2013-14', 'team_code': 'SAS'},\n",
        "    {'year': '2012-13', 'team_code': 'MIA'},\n",
        "    {'year': '2011-12', 'team_code': 'MIA'},\n",
        "    {'year': '2010-11', 'team_code': 'DAL'},\n",
        "    {'year': '2009-10', 'team_code': 'LAL'},\n",
        "    {'year': '2008-09', 'team_code': 'LAL'},\n",
        "    {'year': '2007-08', 'team_code': 'BOS'},\n",
        "    {'year': '2006-07', 'team_code': 'SAS'},\n",
        "    {'year': '2005-06', 'team_code': 'MIA'},\n",
        "    {'year': '2004-05', 'team_code': 'SAS'},\n",
        "    {'year': '2003-04', 'team_code': 'DET'},\n",
        "    {'year': '2002-03', 'team_code': 'SAS'},\n",
        "    {'year': '2001-02', 'team_code': 'LAL'},\n",
        "    {'year': '2000-01', 'team_code': 'LAL'},\n",
        "    {'year': '1999-00', 'team_code': 'LAL'},\n",
        "    {'year': '1998-99', 'team_code': 'SAS'},\n",
        "    {'year': '1997-98', 'team_code': 'CHI'},\n",
        "    {'year': '1996-97', 'team_code': 'CHI'},\n",
        "    {'year': '1995-96', 'team_code': 'CHI'},\n",
        "    {'year': '1994-95', 'team_code': 'HOU'},\n",
        "]\n",
        "def get_team_id(team_code):\n",
        "    nba_teams = teams.get_teams()\n",
        "    for team in nba_teams:\n",
        "        if team['abbreviation'] == team_code:\n",
        "            return team['id']\n",
        "    return None\n",
        "def get_championship_rosters(champions_list):\n",
        "    all_rosters = []\n",
        "    for i, champ in enumerate(champions_list):\n",
        "        season = champ['year']\n",
        "        team_code = champ['team_code']\n",
        "        team_id = get_team_id(team_code)\n",
        "        if team_id:\n",
        "            try:\n",
        "                roster_endpoint = commonteamroster.CommonTeamRoster(season=season, team_id=team_id)\n",
        "                roster_df = roster_endpoint.get_data_frames()[0]\n",
        "                roster_df['CHAMPIONSHIP_SEASON'] = season\n",
        "                roster_df['TEAM_CODE'] = team_code\n",
        "                all_rosters.append(roster_df)\n",
        "                time.sleep(0.5)\n",
        "            except Exception as e:\n",
        "                print(f\"Hata ({season}): {e}\")\n",
        "    if all_rosters:\n",
        "        return pd.concat(all_rosters, ignore_index=True)\n",
        "    return pd.DataFrame()\n",
        "df = get_championship_rosters(champions_map)\n",
        "if not df.empty:\n",
        "    display(df[['CHAMPIONSHIP_SEASON', 'TEAM_CODE', 'PLAYER', 'AGE', 'SCHOOL']].head())\n",
        "    display(df[['CHAMPIONSHIP_SEASON', 'TEAM_CODE', 'PLAYER', 'AGE', 'SCHOOL']].tail())"
      ]
