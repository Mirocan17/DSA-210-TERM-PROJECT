      "source": [
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import pandas as pd\n",
        "from nba_api.stats.endpoints import drafthistory\n",
        "print(\"Fetching draft history, please wait...\")\n",
        "draft = drafthistory.DraftHistory(league_id='00', season_year_nullable='')\n",
        "draft_df = draft.get_data_frames()[0]\n",
        "draft_df.rename(columns={'PERSON_ID': 'PLAYER_ID', 'TEAM_ID': 'DRAFT_TEAM_ID', 'OVERALL_PICK': 'PICK_NUMBER'}, inplace=True)\n",
        "merged_df = pd.merge(df, draft_df[['PLAYER_ID', 'DRAFT_TEAM_ID', 'PICK_NUMBER', 'SEASON']], on='PLAYER_ID', how='left')\n",
        "homegrown_df = merged_df[merged_df['TeamID'] == merged_df['DRAFT_TEAM_ID']]\n",
        "top_picks_df = homegrown_df[homegrown_df['PICK_NUMBER'] <= 10]\n",
        "plot_data = top_picks_df.groupby(['CHAMPIONSHIP_SEASON', 'TEAM_CODE']).size().reset_index(name='COUNT')\n",
        "plt.figure(figsize=(16, 8))\n",
        "sns.set_style(\"whitegrid\")\n",
        "sns.barplot(x='CHAMPIONSHIP_SEASON', y='COUNT', data=plot_data, hue='TEAM_CODE', dodge=False, palette='viridis')\n",
        "plt.title(\"Impact of 'Tanking': Homegrown Top-10 Draft Picks in Championship Rosters\", fontsize=16, fontweight='bold')\n",
        "plt.xlabel('Season', fontsize=12)\n",
        "plt.ylabel('Number of Homegrown Top-10 Players', fontsize=12)\n",
        "plt.xticks(rotation=45)\n",
        "plt.legend(title='Champion Team', bbox_to_anchor=(1.05, 1), loc='upper left')\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "print(\"\\n--- Examples of Homegrown Top-10 Picks in Championship Teams ---\")\n",
        "cols = ['CHAMPIONSHIP_SEASON', 'TEAM_CODE', 'PLAYER', 'PICK_NUMBER', 'SCHOOL']\n",
        "display(top_picks_df[cols].sort_values(by='CHAMPIONSHIP_SEASON', ascending=False).head(10))"
